<%= form_for @symptom do |f| %>

  <% if @symptom.errors.any? %>
   <div id="error_explanation">
     <h2><%= pluralize(@symptom.errors.count, "error") %>
     prohibited this symptom from being saved:</h2>
     <ul>
     <% @symptom.errors.full_messages.each do |msg| %>
       <li><%= msg %></li>
     <% end %>
     </ul>
   </div>
  <% end %>

  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <%= f.submit %>
<% end %>
